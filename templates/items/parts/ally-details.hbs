<div class="form-group">
  <label>{{localize "PF1RS.GrantedActions"}}</label>
  <div class="form-fields">
    <ul class="traits-list tag-list">
      {{#each actions}}
      <li class="tag">{{ this }}</li>
      {{/each}}
    </ul>
    <a
      class="trait-selector"
      data-title="PF1RS.ActionSelector"
      data-options="rebellionActions"
      data-for="system.rActions"
    >
      <i class="fas fa-pen-to-square"></i>
    </a>
  </div>
</div>
